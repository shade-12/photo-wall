{
  "name": "photo-wall",
  "version": "1.0.0",
  "description": "A photo wall created using React, Typescript, Express and AWS S3.",
  "main": "server.js",
  "dependencies": {
    "aws-sdk": "^2.747.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "scripts": {
    "build": "cd client && npm install && npm run build && cd ..",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shade-12/photo-wall.git"
  },
  "author": "Shade Wong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shade-12/photo-wall/issues"
  },
  "homepage": "https://github.com/shade-12/photo-wall#readme"
}
